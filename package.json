{
  "name": "pomodoro",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "commit": "git-cz",
    "lint": "eslint --cache .",
    "format": "prettier --write '*.{html,js,json,md}'"
  },
  "dependencies": {
    "@material-ui/styles": "^4.9.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@qbaepst/prettier-config": "^1.3.0",
    "@testing-library/dom": "^7.2.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.0",
    "typescript": "^3.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 52,
        "branches": 65,
        "functions": 60,
        "lines": 51
      }
    }
  },
  "lint-staged": {
    "*.{html,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "pettier": "@qbaepst/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/quibaritaenperdresatrompe/pomodoro"
  }
}
